#this is using dictionary and TC:O(N) :it can handle both positive and negative cases

arr = list(map(int, input().split()))
k = 3
sum = 0
maxlen = 0
start = -1
end = -1
d = {}

for i in range(len(arr)):
    sum += arr[i]
    if sum == k:
        maxlen = i + 1
        start = 0
        end = i
    rem = sum - k
    if rem in d:
        length = i - d[rem]
        if length > maxlen:
            maxlen = length
            start = d[rem] + 1
            end = i
    if sum not in d:  # Only store the first occurrence of each sum
        d[sum] = i

print(maxlen)
if start != -1 and end != -1:
    print(arr[start:end + 1])
else:
    print("No subarray found")
